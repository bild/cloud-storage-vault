<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
android:id="@+id/widget28"
android:layout_width="fill_parent"
android:layout_height="fill_parent"
xmlns:android="http://schemas.android.com/apk/res/android"
android:orientation="vertical"
>
<TextView
android:id="@+id/manualshare_header"
android:layout_width="wrap_content"
android:layout_height="wrap_content"
android:text="Import Share"
android:layout_gravity="center"
android:textStyle="bold"
android:singleLine="true"
android:textSize="25sp"
>
</TextView>
<TextView
android:id="@+id/manualshareactivity_usernameheader"
android:layout_width="wrap_content"
android:layout_height="wrap_content"
android:text="Enter nickname:"
>
</TextView>
<EditText
android:id="@+id/manualshareactivity_usernameedittext"
android:layout_width="fill_parent"
android:layout_height="wrap_content"
>
</EditText>
<TextView
android:id="@+id/manualshareactivity_keyheader"
android:layout_width="wrap_content"
android:layout_height="wrap_content"
android:text="Enter Key:"
>
</TextView>
<EditText
android:id="@+id/manualshareactivity_keyedittext"
android:layout_width="fill_parent"
android:layout_height="wrap_content"
>
</EditText>
<Button
android:id="@+id/manualshareactivity_processbutton"
android:layout_width="fill_parent"
android:layout_height="wrap_content"
android:text="Import"
>
</Button>
</LinearLayout>
