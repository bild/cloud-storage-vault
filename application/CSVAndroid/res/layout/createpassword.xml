<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
android:id="@+id/widget28"
android:layout_width="fill_parent"
android:layout_height="fill_parent"
xmlns:android="http://schemas.android.com/apk/res/android"
android:orientation="vertical"
>
<TextView
android:id="@+id/setpassword_header"
android:layout_width="wrap_content"
android:layout_height="wrap_content"
android:text="Keyring Password"
android:layout_gravity="center"
android:textStyle="bold"
android:singleLine="true"
android:textSize="25sp"
>
</TextView>
<TextView
android:id="@+id/setpassword_header"
android:layout_width="wrap_content"
android:layout_height="wrap_content"
android:text="Enter New Password:"
>
</TextView>
<EditText
android:id="@+id/setpassword_new"
android:layout_width="fill_parent"
android:layout_height="wrap_content"
android:password="true"
>
</EditText>
<TextView
android:id="@+id/setpassword_confheader"
android:layout_width="wrap_content"
android:layout_height="wrap_content"
android:text="Confirm Password:"
>
</TextView>
<EditText
android:id="@+id/setpassword_confnew"
android:layout_width="fill_parent"
android:layout_height="wrap_content"
android:password="true"
>
</EditText>
<Button
android:id="@+id/setpassword_okbutton"
android:layout_width="fill_parent"
android:layout_height="wrap_content"
android:text="Ok"
>
</Button>
<Button
android:id="@+id/setpassword_cancelbutton"
android:layout_width="fill_parent"
android:layout_height="wrap_content"
android:text="Cancel"
>
</Button>
</LinearLayout>
