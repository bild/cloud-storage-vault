<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
android:id="@+id/widget27"
android:layout_width="70pt"
android:layout_height="fill_parent"
xmlns:android="http://schemas.android.com/apk/res/android"
android:orientation="vertical"
>
<TextView
android:id="@+id/enteronlinecredentials_usernameheader"
android:layout_width="wrap_content"
android:layout_height="wrap_content"
android:text="Enter username:"
>
</TextView>
<EditText
android:id="@+id/enteronlinecredentials_username"
android:layout_width="fill_parent"
android:layout_height="wrap_content"
android:text=""
>
</EditText>
<TextView
android:id="@+id/enteronlinecredentials_passwordheader"
android:layout_width="wrap_content"
android:layout_height="wrap_content"
android:text="Enter password"
>
</TextView>
<EditText
android:id="@+id/enteronlinecredentials_password"
android:layout_width="fill_parent"
android:layout_height="wrap_content"
android:text=""
>
</EditText>
<LinearLayout
        android:orientation="horizontal"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:gravity="right">
		    <Button android:id="@+id/enteronlinecredentials_cancel"
		        android:layout_width="35pt"
		        android:layout_height="wrap_content"
		        android:text="Cancel"/>
		    <Button android:id="@+id/enteronlinecredentials_ok"
		        android:layout_width="35pt"
		        android:layout_height="wrap_content"
		        android:text="Ok"/>
	    </LinearLayout>
</LinearLayout>
